# Mod1 (usually aka "Alt" or "Meta")
# Mod2 ("NumLock")
# Mod3 (unused on default QWERTY layout)
# Mod4 (aka "Super" or "Windows-Key")
# Mod5 (e.g. "Alt_Gr" on german QWERTZ layout)
# Shift
# Control 
# Mode_Switch

set $alt Mod1
set $shift Shift
set $ctrl Control 
set $super Mod4

set $mod Mod4

exec ~/.config/scripts/autostart.sh
exec_always --no-startup-id ~/.config/scripts/always.sh

include ~/.config/i3/config.d/*
